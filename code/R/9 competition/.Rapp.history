calcLVcomp <- function(a12, a21, K1, K2) {#
	N1star = (K1 - a12 * K2)/(1 - a12 * a21)#
	N2star = (K2 - a21 * K1)/(1 - a12 * a21)#
#
	yini <- round(c(N1 = runif(1, 0.5, K1), N2 = runif(1, 1, K2))#
#
	outLVa <- data.frame(ode(yini, times, LVcomp1, parms = c(K1, K2, r1, r2, a12, a21)))#
	outLVb <- expand.grid(N1 = seq(1, max(K1, outLVa$N1), length = 20), N2 = seq(1, max(K2, #
		outLVa$N2), length = 20), N1_end = NA, N2_end = NA)#
	for (i in 1:nrow(outLVb)) {#
		outLVb[i, 3:4] <- ode(c(N1 = outLVb$N1[i], N2 = outLVb$N2[i]), times = 0:1, LVcomp1, #
			parms = c(K1, K2, r1, r2, a12, a21))[2, 2:3]#
	}#
	return(list(predictions = outLVa, phase_plane = outLVb, equilibria = c(N1star = N1star, #
		N2star = N2star), initial_pop_size = yini))#
}
calcLVcomp <- function(a12, a21, K1, K2) {#
	N1star = (K1 - a12 * K2)/(1 - a12 * a21)#
	N2star = (K2 - a21 * K1)/(1 - a12 * a21)#
#
	yini <- round(c(N1 = runif(1, 1, K1), N2 = runif(1, 1, K2)))#
#
	outLVa <- data.frame(ode(yini, times, LVcomp1, parms = c(K1, K2, r1, r2, a12, a21)))#
	outLVb <- expand.grid(N1 = seq(1, max(K1, outLVa$N1), length = 20), N2 = seq(1, max(K2, #
		outLVa$N2), length = 20), N1_end = NA, N2_end = NA)#
	for (i in 1:nrow(outLVb)) {#
		outLVb[i, 3:4] <- ode(c(N1 = outLVb$N1[i], N2 = outLVb$N2[i]), times = 0:1, LVcomp1, #
			parms = c(K1, K2, r1, r2, a12, a21))[2, 2:3]#
	}#
	return(list(predictions = outLVa, phase_plane = outLVb, equilibria = c(N1star = N1star, #
		N2star = N2star), initial_pop_size = yini))#
}
