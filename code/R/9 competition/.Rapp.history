source("http://manuplants.org/BIOU9CE/Practical_4.R")#
# Global variable#
n.runs <- 2000#
#
# model A: 3 spp food chain: Producer, consumer, predator#
mA <- expression({#
	n.sp <- 3#
	M <- matrix(0, nrow = n.sp, ncol = n.sp)#
	M[1, 1] <- -1#
	M[1, 2] <- runif(1, -10, 0)#
	M[2, 1] <- runif(1, 0, 0.1)#
	M[2, 3] <- runif(1, -10, 0)#
	M[3, 2] <- runif(1, 0, 0.1)#
})#
Model_A <- M_tester(mA)#
M_plotter(Model_A, model = mA)
